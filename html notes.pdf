Nested list
<!-- Nested lists are created by placing <ul> or <ol> (ordered list) tags inside <li> tags -->

<ul>
    <!-- Frontend section -->
    <li>
        Frontend
        <!-- Nested unordered list for frontend technologies -->
        <ul>
            <!-- HTML section -->
            <li>HTML
                <!-- Nested ordered list for HTML versions -->
                <ol>
                    <li>HTML5</li>
                </ol>
            </li>
            <!-- CSS section -->
            <li>CSS
                <!-- Nested ordered list for CSS versions and Bootstrap -->
                <ol>
                    <li>CSS3</li>
                    <li>Bootstrap</li>
                </ol>
            </li>
            <!-- JavaScript section -->
            <li>JavaScript
                <!-- Nested ordered list for JavaScript frameworks/libraries -->
                <ol>
                    <li>jQuery</li>
                </ol>
            </li>
        </ul>
    </li>

    <!-- Backend section -->
    <li>
        Backend
        <!-- Nested unordered list for backend technologies -->
        <ul>
            <!-- PHP section -->
            <li>PHP</li>
            <!-- Database section -->
            <li>Database</li>
        </ul>
    </li>
</ul>

