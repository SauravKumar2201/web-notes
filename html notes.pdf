HTML TAble Tag
<!-- The <table> tag is used to create a table -->
<table border="1px">
  <!-- The <thead> tag defines the head of the table -->
  <thead>
    <!-- The <tr> tag defines a new row in the table head -->
    <tr>
      <!-- The <th> tag is used to show the heading of each column in the table row -->
      <th>Semester</th>
      <th>Fees</th>
    </tr>
  </thead>
  
  <!-- The <tbody> tag defines the body of the table -->
  <tbody>
    <!-- The <tr> tag defines a new row in the table body -->
    <tr>
      <!-- The <td> tag is used to define table data -->
      <td>5th</td>
      <td>100k</td>
    </tr>
    <tr>
      <td>6th</td>
      <td>200k</td>
    </tr>
  </tbody>
</table>

<!-- Table Attributes -->
<!-- bgcolor sets the background color of the table -->
<!-- align sets the alignment of the table -->
<!-- border sets the border width of the table -->
<!-- cellspacing sets the space between cells on the outer side of the table -->
<!-- cellpadding sets the space between the content of a cell and its border -->
<!-- width sets the width of the table -->
<!-- height sets the height of the table -->
<table bgcolor="red" align="center" border="10" cellspacing="5" cellpadding="5" width="500" height="500"></table>

<!-- Row Attributes -->
<!-- bgcolor sets the background color of the table row and overrides the table's bgcolor -->
<!-- align sets the alignment of the content within the table row -->
<tr bgcolor="yellow" align="center"></tr>

<!-- Cell Attributes -->
<!-- bgcolor sets the background color of the table cell -->
<!-- colspan spans a cell across multiple columns -->
<!-- rowspan spans a cell across multiple rows -->
<td bgcolor="red" colspan="2"></td>
<td bgcolor="green" rowspan="2"></td>

